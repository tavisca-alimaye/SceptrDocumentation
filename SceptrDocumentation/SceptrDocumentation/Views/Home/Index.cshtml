@{
    ViewBag.Title = "Scepter Documentation Tool";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
            </hgroup>
            <p>
                Documentation tool for supplier and client mapping. Also supports supplier questionnaire.
